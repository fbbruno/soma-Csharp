using System;

class MainClass {
  public static void Main (string[] args) {
    int n1, n2, soma;

    Console.Write("n1: ");
    n1 = int.Parse(Console.ReadLine());
    Console.Write("n2: ");
    n2 = int.Parse(Console.ReadLine());
   
    soma = n1 + n2;

    Console.WriteLine("Resultado da soma de {0} mais {1} Ã©: {2}", n1, n2, soma);
  }
}
